{
  "compilerOptions": {
      "target": "es2020",
      "module": "system",
      "outFile": "../client-js/index.js",
      "strict": false,
      "esModuleInterop": true,//将 CommonJS/AMD/UMD 模块视为与 ES6 模块类似
      "skipLibCheck": true,//跳过生命文件的检查
      "forceConsistentCasingInFileNames": true,//不区分大小写
      // "sourceMap": true,
      "baseUrl": "./", // types文件夹的相对路径
      "paths": {
          "*": ["types/*"]
      },
      "declaration": true,
      "lib": ["ES2020", "DOM"]
  },
}